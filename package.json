{
  "name": "jpeg-buffer-orientation",
  "version": "4.1.1",
  "description": "Get orientation of JPEG file.",
  "homepage": "https://github.com/fisker/jpeg-buffer-orientation#readme",
  "bugs": {
    "url": "https://github.com/fisker/jpeg-buffer-orientation/issues"
  },
  "repository": "fisker/jpeg-buffer-orientation",
  "funding": "https://github.com/fisker/jpeg-buffer-orientation?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "main": "dist/index.cjs",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.min.mjs",
  "browser": "dist/index.min.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist docs",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "docs": "run-p docs:*",
    "docs:html": "cpy src/index.html docs",
    "docs:image": "cpy __tests__/fixture/** docs/images",
    "docs:js": "cpy dist/** docs/dist",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,cjs,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build test docs format dist",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@commitlint/cli": "17.2.0",
    "@fisker/commitlint-config": "2.0.5",
    "@fisker/eslint-config": "10.2.4",
    "@fisker/eslint-config-jest": "3.0.12",
    "@fisker/husky-config": "4.1.2",
    "@fisker/lint-staged-config": "3.1.5",
    "@fisker/prettier-config": "5.0.6",
    "@rollup/plugin-babel": "6.0.2",
    "babel-jest": "29.3.0",
    "core-js": "3.26.0",
    "cpy-cli": "4.2.0",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "5.0.0",
    "eslint": "8.27.0",
    "esm-utils": "4.1.0",
    "fast-glob": "3.2.12",
    "husky": "8.0.2",
    "jest": "29.3.0",
    "lint-staged": "13.0.3",
    "markdownlint-cli": "0.32.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "rollup": "3.2.5",
    "rollup-plugin-prettier": "3.0.0",
    "rollup-plugin-terser": "7.0.2",
    "sort-package-json": "2.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
